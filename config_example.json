{
    "Name":"OPCUA Gateway",
	"Version":"1.0",
	"Author":"SuXin",
	"Date":"2020-04-25",
	"Summary":"An example configuration file of OPC UA gateway, reference to https://github.com/suxin1412/modus_gate by shen",
	"Port": 4840,
	"AddressSpace":[
		{
			"NodeID_NamespaceIndex":1,
			"NodeID_IDType":"String",
			"NodeID_ID":"KFR-35GW",
			"NodeClass":"Object",
			"BrowseName":"Air Conditioner",
			"DisplayName":"Air Conditioner KFR-35GW",
			"Description":"air conditioner with a temperature sensor",
			"ParentNodeId":"ObjectsFolder",
			"ReferenceTypeId":"Organizes",
			"QualifiedName":"Air Conditioner",
			"Communication":"Serial",
			"DeviceLocation":"/dev/ttyUSB0",
			"Baud":9600,
			"Parity":"N",
			"DataBits":8,
			"StopBits":1,
			"ChildNode":[
				{
					"NodeID_NamespaceIndex":1,
					"NodeID_IDType":"String",
					"NodeID_ID":"Temp-1",
					"NodeClass":"Variable",
					"BrowseName":"Indoor Temperature",
					"DisplayName":"Indoor Temperature Temp-1",
					"Description":"temperature sensor on the air conditioner",
					"ReferenceTypeId":"HasProperty",
					"QualifiedName":"Indoor Temperature",
					"DataType":"Double",
					"InitialValue":12.0,
					"BottomProtocolType":"Modbus",
					"MB_MachineAddress":1,
					"MB_RegisterAddress":0,
					"MB_FunctionCode":3,
					"MB_DataBitSize":16,
					"RefreshFrequency":1000,
					"BottomDataType":"uint16_t",
					"MappingCoefficient":1.0
				},{
					"NodeID_NamespaceIndex":1,
					"NodeID_IDType":"String",
					"NodeID_ID":"Temp-2",
					"NodeClass":"Variable",
					"BrowseName":"Setted Temperature",
					"DisplayName":"Setted Temperature Temp-2",
					"Description":"setted temperature of the air conditioner",
					"ReferenceTypeId":"HasProperty",
					"QualifiedName":"Setted Temperature",
					"DataType":"Double",
					"InitialValue":13.1,
					"BottomProtocolType":"Modbus",
					"MB_MachineAddress":1,
					"MB_RegisterAddress":1,
					"MB_FunctionCode":3,
					"MB_DataBitSize":16,
					"RefreshFrequency":1000,
					"BottomDataType":"uint16_t",
					"MappingCoefficient":1.0
				}
			]
		}
	]
}
